                sling:resourceType="foundation/components/table"
                tableData="&lt;table width=&quot;100%&quot; cellspacing=&quot;0&quot; cellpadding=&quot;1&quot; border=&quot;1&quot;>&#xa;&lt;tbody>&lt;tr>&lt;td>&amp;nbsp;&lt;/td>&#xa;&lt;td>&lt;p style=&quot;text-align: center;&quot;>Feature Group - 1&amp;nbsp; &amp;nbsp;&lt;/p>&#xa;&lt;p style=&quot;text-align: center;&quot;>Custom test automation&amp;nbsp;&lt;/p>&#xa;&lt;p style=&quot;text-align: center;&quot;>&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp;GUI&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp;&amp;nbsp;&lt;/p>&#xa;&lt;/td>&#xa;&lt;td>&lt;p style=&quot;text-align: center;&quot;>Feature Group - 2&amp;nbsp;&lt;/p>&#xa;&lt;p style=&quot;text-align: center;&quot;>Automated report generation&lt;/p>&#xa;&lt;p style=&quot;text-align: center;&quot;>&amp;amp; API's&lt;/p>&#xa;&lt;/td>&#xa;&lt;td>&lt;p style=&quot;text-align: center;&quot;>Feature Group - 3&amp;nbsp;&lt;/p>&#xa;&lt;p style=&quot;text-align: center;&quot;>Database connectivity and&lt;/p>&#xa;&lt;p style=&quot;text-align: center;&quot;>automation&lt;/p>&#xa;&lt;/td>&#xa;&lt;/tr>&lt;tr>&lt;td style=&quot;text-align: center;&quot;>Cumulative Investment(hrs)&lt;/td>&#xa;&lt;td style=&quot;text-align: center;&quot;>&amp;nbsp; 2500&lt;/td>&#xa;&lt;td style=&quot;text-align: center;&quot;>200&lt;/td>&#xa;&lt;td style=&quot;text-align: center;&quot;>675&lt;/td>&#xa;&lt;/tr>&lt;tr>&lt;td style=&quot;text-align: center;&quot;>Cost of Investment(avg - 50 USD/hr)&amp;nbsp;&lt;/td>&#xa;&lt;td style=&quot;text-align: center;&quot;>USD 1,25,000&lt;/td>&#xa;&lt;td style=&quot;text-align: center;&quot;>USD 10,000&lt;/td>&#xa;&lt;td style=&quot;text-align: center;&quot;>USD 33,750&lt;/td>&#xa;&lt;/tr>&lt;tr>&lt;td style=&quot;text-align: center;&quot;>Time saving per instance of usage&lt;/td>&#xa;&lt;td style=&quot;text-align: center;&quot;>6&lt;/td>&#xa;&lt;td style=&quot;text-align: center;&quot;>2&lt;/td>&#xa;&lt;td style=&quot;text-align: center;&quot;>1&lt;/td>&#xa;&lt;/tr>&lt;tr>&lt;td>&lt;p style=&quot;text-align: center;&quot;>cumulative savings for 2500 instances&lt;/p>&#xa;&lt;p style=&quot;text-align: center;&quot;>of usage across multiple projects&lt;/p>&#xa;&lt;/td>&#xa;&lt;td style=&quot;text-align: center;&quot;>15,000&lt;/td>&#xa;&lt;td style=&quot;text-align: center;&quot;>5,000&lt;/td>&#xa;&lt;td style=&quot;text-align: center;&quot;>2,500&lt;/td>&#xa;&lt;/tr>&lt;tr>&lt;td style=&quot;text-align: center;&quot;>cumulative cost savings&lt;/td>&#xa;&lt;td style=&quot;text-align: center;&quot;>USD 1125000&lt;/td>&#xa;&lt;td style=&quot;text-align: center;&quot;>USD 375000&lt;/td>&#xa;&lt;td style=&quot;text-align: center;&quot;>USD 1875000&lt;/td>&#xa;&lt;/tr>&lt;tr>&lt;td style=&quot;text-align: center;&quot;>ROI&lt;/td>&#xa;&lt;td style=&quot;text-align: center;&quot;>800%&lt;/td>&#xa;&lt;td style=&quot;text-align: center;&quot;>365%&lt;/td>&#xa;&lt;td style=&quot;text-align: center;&quot;>455%&lt;/td>&#xa;&lt;/tr>&lt;/tbody>&lt;/table>&#xa;"
                textIsRich="true"/>
            <text_791327655
                jcr:created="{Date}2019-05-20T11:53:45.999+05:30"
                jcr:createdBy="admin"
                jcr:lastModified="{Date}2019-05-24T09:44:40.772+05:30"
